<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.masdmtr.skillsmonster.entity.SourceSite" table="source_site" schema="public"
           catalog="skillsmonster">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="url">
            <column name="url" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="apiUrl">
            <column name="api_url" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>